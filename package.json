{
  "name": "egg-format-logger",
  "version": "1.0.0",
  "description": "An egg plugin for formatting logs. This plugin and [egg-logger](https://github.com/eggjs/egg-logger) to work together.",
  "eggPlugin": {
    "name": "formatLogger",
    "env": [
      "local",
      "test",
      "unittest",
      "prod"
    ]
  },
  "files": [
    "app/**",
    "app.js",
    "lib/**"
  ],
  "scripts": {
    "ii": "yarn && cd example && yarn",
    "build": "tsc",
    "lk": "yarn link && cd example && yarn link egg-format-logger ",
    "dev": "yarn lk && cd example && yarn dev"
  },
  "dependencies": {
    "egg-ts-helper": "^1.25.9",
    "js-shortid": "^0.1.1"
  },
  "keywords": [
    "egg",
    "egg-plugin",
    "eggPlugin",
    "egg-logger",
    "logger",
    "format",
    "format-logger",
    "egg-format-logger"
  ],
  "devDependencies": {
    "egg": "^2.29.4",
    "egg-logger": "^2.6.1",
    "typescript": "^4.2.4"
  }
}
